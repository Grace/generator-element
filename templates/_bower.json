{
    "name": "<%= githubRepo %>",
    "version": "0.0.0",
    "description": "<%= elementDescription %>",
    "license": "MIT",
    "main": "<%= elementName %>.html",
    "keywords": [<% if (boilerplate == 'Polymer') { %>
        "polymer",<% } %><% if (boilerplate == 'X-Tag') { %>
        "x-tag",<% } %>
        "web-components"
    ],
    "ignore": [
        "**/.*",
        "bower_components"
    ],
    "dependencies": {<% if (boilerplate == 'VanillaJS') { %>
        "webcomponentsjs": "^0.7.0"<% } %><% if (boilerplate == 'Polymer') { %>
        "polymer": "Polymer/polymer#^0.9.0"<% } %><% if (boilerplate == 'X-Tag') { %>
        "x-tag-core": "^1.0.14"<% } %>
    }
}
