{
    "name": "<%= githubRepo %>",
    "version": "0.0.0",
    "description": "<%= elementDescription %>",
    "license": "MIT",<% if (solution == 'VanillaJS') { %>
    "main": "src/<%= elementName %>.html",<% } else { %>
    "main": "dist/<%= elementName %>.html",<% } %>
    "keywords": [<% if (solution == 'Polymer') { %>
        "polymer",<% } %><% if (solution == 'X-Tag') { %>
        "x-tag",<% } %>
        "web-components"
    ],
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components"
    ],
    "dependencies": {<% if (solution == 'Polymer') { %>
        "polymer": "Polymer/polymer#~0.3.3"<% } %><% if (solution == 'X-Tag') { %>
        "platform": "Polymer/platform#~0.3.3",
        "x-tag-core": "~0.9.7"<% } %><% if (solution == 'VanillaJS') { %>
        "platform": "Polymer/platform#~0.3.3"<% } %>
    }
}
